{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "standard",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jest/recommended",
    "plugin:n/recommended",
    "plugin:prettier/recommended",
    "plugin:promise/recommended"
  ],
  "plugins": ["jest", "prettier"],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "space-before-function-parent": 0
  }
}
